{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "identity-roles-restore-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/roles/{id}/restore"
        },
        "Transforms": [
          {
            "PathPattern": "/api/roles/{id}/restore"
          }
        ]
      },
      "identity-roles-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/roles/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/roles/{**catch-all}"
          }
        ]
      },
      "identity-roles-root-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/roles"
        },
        "Transforms": [
          {
            "PathPattern": "/api/roles"
          }
        ]
      },
      "identity-auth-route": {
        "ClusterId": "identity-cluster",
        "RateLimiterPolicy": "fixed-window",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/auth/{**catch-all}"
          }
        ]
      },
      "identity-users-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/users/{**catch-all}"
          }
        ]
      },
      "identity-users-root-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/users"
        },
        "Transforms": [
          {
            "PathPattern": "/api/users"
          }
        ]
      },
      "identity-institutions-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/institutions/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/institutions/{**catch-all}"
          }
        ]
      },
      "identity-students-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/students/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/students/{**catch-all}"
          }
        ]
      },
      "identity-teachers-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/teachers/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/teachers/{**catch-all}"
          }
        ]
      },
      "coaching-assignments-route": {
        "ClusterId": "coaching-cluster",
        "Match": {
          "Path": "/api/assignments/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/assignments/{**catch-all}"
          }
        ]
      },
      "coaching-exams-route": {
        "ClusterId": "coaching-cluster",
        "Match": {
          "Path": "/api/exams/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/exams/{**catch-all}"
          }
        ]
      },
      "coaching-goals-route": {
        "ClusterId": "coaching-cluster",
        "Match": {
          "Path": "/api/goals/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/goals/{**catch-all}"
          }
        ]
      },
      "coaching-sessions-route": {
        "ClusterId": "coaching-cluster",
        "Match": {
          "Path": "/api/sessions/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/sessions/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity-service:8080"
          }
        }
      },
      "coaching-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://coaching-service:8080"
          }
        }
      }
    }
  }
}